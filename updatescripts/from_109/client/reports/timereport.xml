 <querysource>
  <name>detail</name>
  <sql>select tehead_number as sheet_number,formatdate(tehead_weekending) as weekending,
item_number,item_descrip1,formatdate(teitem_workdate) as workdate,
emp_code,teitem_qty as qty,teitem_rate as rate,
teitem_total as total,teitem_notes as notes
from te.tehead,te.teitem,emp,item
where tehead_id = teitem_tehead_id
and teitem_emp_id = emp_id
and teitem_item_id = item_id
and teitem_type = 'T'
and tehead_id = &lt;? value("headid") ?>
order by workdate ;</sql>
 </querysource>
 <querysource>
  <name>head</name>
  <sql>select (cntct_first_name || ' ' || cntct_last_name) as name,tehead_number as sheet_number,formatdate(tehead_weekending) as weekending,
emp_code
from te.tehead,te.teitem,emp,cntct
where tehead_id = teitem_tehead_id
and emp_cntct_id = cntct_id
and teitem_emp_id = emp_id
and teitem_type = 'T'
and tehead_id = &lt;? value("headid") ?> 
limit 1;</sql>
 </querysource>
